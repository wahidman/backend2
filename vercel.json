{
    "version": 2,
    "builds": [
        "commands": [
          "npm install pg",
          "npm run build"
        ]
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "server.js" }
    ],
    "installCommand": "npm install pg"
  }
  
